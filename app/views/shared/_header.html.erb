<div class="header">
  <div class="container clearfix">
    <a class="header-logo-wordmark" href="/">
      企业Wifi服务平台
    </a>
    <ul id="user-links">
      <% if current_user %>
        <li><%= link_to current_user.name %></li>
        <li><%= link_to "退出", mlogout_path, method: "delete" %></li>
      <% else %>
        <li><%= link_to "注册", msignup_path %></li>
        <li><%= link_to "登录", mlogin_path %></li>
      <% end %>
    </ul>
  </div>  
</div>

